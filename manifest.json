{
  "manifest_version": 3,
  "name": "Python Module Explorer",
  "version": "1.0",
  "description": "Interactive Python module explorer with virtual environment support",
  "permissions": [
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Python Module Explorer"
  },
  "icons": {
    "48": "data:image/svg+xml;base64,"
  }
}
